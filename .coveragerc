[run]
branch = True
cover_pylib = False
include = cdff_dev/*
omit =
    *setup.py


[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    if self.verbose
    if verbose
    def plot
    def _plot
    def print
    @abstractmethod


[html]
directory = cover